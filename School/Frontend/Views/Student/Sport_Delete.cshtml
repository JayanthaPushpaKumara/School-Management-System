
@{
    ViewBag.Title = "Sport_Record_Delete";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}



<body>

    <script language="javascript" type="text/javascript">

        window.onload = function () {

            document.getElementById('submit').onclick = function () {

                {
                    var y = "@ViewBag.detailsid";

                    var x = 'http://localhost:56412/api/sport_event/Delsport_event/' + y;

                   // alert(x);

                    $.ajax({
                        url: x,
                        type: 'GET',
                        dataType: "json",
                        contentType: 'application/json; charset=utf-8',

                        success: function (data) {

                            console.log(data);
                            window.location = "http://localhost:56448/Student/Sport_View/1";
                        },

                        error: function () {

                            alert("error");

                        }
                    });
                }
            }
        }

    </script>

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"></h1>

        </div>
    </div>


    <h2>Are you sure delete this data.. ??</h2>

    <button type="button" class="btn btn-success" id="submit">Delete</button>

    <input type="button" class="btn btn-info" value="Cancel" onclick="location.href = 'http://localhost:56448/Student/Sport_View/1';" />

</body>

